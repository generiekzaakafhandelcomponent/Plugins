{
  "display": "form",
  "components": [
    {
      "key": "result",
      "type": "hidden",
      "label": "Result",
      "properties": {
        "sourceKey": "doc:result"
      },
      "attributes": {
        "data-testid": "notify-nl.summary-result"
      }
    },
    {
      "key": "html",
      "tag": "div",
      "type": "htmlelement",
      "input": false,
      "label": "HTML",
      "content": "<section><h3>NotifyNL Plugin</h3> {{ data ? `<h4>Result</h4><dl class='row mb-5'><dt class='col-12'><pre>${data.result} {{ JSON.stringify(data.result, null, 2) }}</pre></dt></dl>` : 'Nothing...' }}</section><style>pre{padding: 10px; overflow: auto; white-space: pre-wrap; min-height: 100px; resize: both;} .widget-field__field-value{ white-space: pre;}</style>",
      "tableView": false,
      "refreshOnChange": false
    }
  ]
}