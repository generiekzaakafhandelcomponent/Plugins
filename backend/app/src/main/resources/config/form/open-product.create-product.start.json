      "properties": {
        "targetKey": "pv:/frequentie"
      },
      "type": "select",
      "input": true
    },
    {
      "key": "gepubliceerd",
      "type": "checkbox",
      "input": true,
      "label": "Gepubliceerd",
      "tableView": false,
      "properties": {
        "targetKey": "pv:/gepubliceerd"
      },
      "defaultValue": false,
      "attributes": {
        "data-testid": "open-product.create-product.start-gepubliceerd"
      }
    },
    {
      "key": "submit",
      "type": "button",
      "input": true,
      "label": "Submit",
      "tableView": false,
      "disableOnInvalid": true
    }
