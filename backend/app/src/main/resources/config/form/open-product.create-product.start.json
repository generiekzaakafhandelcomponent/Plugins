    {
      "label": "Status",
      "tableView": true,
      "data": {
        "values": [
          {
            "label": "Initieel",
            "value": "initieel"
          },
          {
            "label": "In_aanvraag",
            "value": "inAanvraag"
          },
          {
            "label": "Gereed",
            "value": "gereed"
          },
          {
            "label": "Actief",
            "value": "actief"
          },
          {
            "label": "Ingetrokken",
            "value": "ingetrokken"
          },
          {
            "label": "Geweigerd",
            "value": "geweigerd"
          },
          {
            "label": "Verlopen",
            "value": "verlopen"
          }
        ]
      },
      "key": "select",
      "properties": {
        "targetKey": "pv:/status"
      },
      "type": "select",
      "input": true
    },
    {
      "label": "Frequentie",
      "widget": "choicesjs",
      "tableView": true,
      "data": {
        "values": [
          {
            "label": "Eenmalig",
            "value": "eenmalig"
          },
          {
            "label": "Maandelijks",
            "value": "maandelijks"
          },
          {
            "label": "Jaarlijks",
