{
  "display": "form",
  "components": [
    {
      "label": "selectedDemo",
      "key": "selectedDemo",
      "type": "hidden",
      "input": true,
      "tableView": false
    },
    {
      "label": "BSN-nummer",
      "tableView": false,
      "validateWhenHidden": false,
      "key": "bsn",
      "conditional": {
        "show": true,
        "when": "selectedDemo",
        "eq": "contactgeschiedenisOphalenBsn"
      },
      "type": "container",
      "input": true,
      "components": [
        {
          "key": "html",
          "tag": "div",
          "type": "htmlelement",
          "attrs": [
            {
              "attr": "",
              "value": ""
            }
          ],
          "input": false,
          "label": "HTML",
          "content": "<section>\n<p><span><strong>BSN-nummer: </strong>{{{ data?.bsn }}}</span></p>\n</section>\n",
          "tableView": false,
          "refreshOnChange": false
        }
      ]
    },
    {
      "label": "Partij",
      "tableView": false,
      "validateWhenHidden": false,
      "key": "partij",
      "conditional": {
        "show": true,
        "when": "selectedDemo",
        "eq": "partijOphalenOfMaken"
      },
      "type": "container",
      "input": true,
      "components": [
        {
          "label": "HTML",
          "tag": "div",
          "attrs": [
            {
              "attr": "",
              "value": ""
            }
          ],
          "content": "<section>\n<p><span><strong>Partij UUID: </strong>{{{ data?.partijUuid }}}</span></p>\n</section>\n",
          "refreshOnChange": false,
          "key": "html",
          "type": "htmlelement",
          "input": false,
          "tableView": false
        }
      ]
    }
  ]
}