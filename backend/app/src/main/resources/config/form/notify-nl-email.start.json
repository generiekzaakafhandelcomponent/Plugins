{
  "display": "form",
  "components": [
    {
      "key": "templateId",
      "type": "textarea",
      "input": true,
      "label": "Template ID",
      "tableView": true,
      "autoExpand": true,
      "properties": {
        "targetKey": "doc:/templateId"
      },
      "applyMaskOn": "change",
      "attributes": {
        "data-testid": "notify-nl-email.start-templateId"
      }
    },
    {
      "key": "recipientEmail",
      "type": "textarea",
      "input": true,
      "label": "Recipient E-mail",
      "tableView": true,
      "autoExpand": true,
      "properties": {
        "targetKey": "doc:/email"
      },
      "applyMaskOn": "change",
      "attributes": {
        "data-testid": "notify-nl-email.start-recipientEmail"
      }
    },
    {
      "label": "Personalisation parameters",
      "tableView": false,
      "key": "dataMap",
      "type": "datamap",
      "input": true,
      "valueComponent": {
        "type": "textfield",
        "key": "value",
        "label": "Value",
        "properties": {
          "targetKey": "doc:/personalisation"
        },
        "input": true,
        "hideLabel": true,
        "tableView": true
      }
    },
    {
      "key": "submit",
      "type": "button",
      "input": true,
      "label": "Submit",
      "tableView": false,
      "disableOnInvalid": true
    }
  ]
}