{
  "compileOnSave": false,
  "compilerOptions": {
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "target": "es2022",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2021", "dom", "dom.iterable"],
    "useDefineForClassFields": false,
    "paths": {
      "@src": ["src"],
      "@src/*": ["src/*"],
      "@valtimo-plugins/externe-klanttaak": ["dist/valtimo-plugins/externe-klanttaak"],
      "@valtimo-plugins/freemarker": ["dist/valtimo-plugins/freemarker"],
      "@valtimo-plugins/haal-centraal": ["dist/valtimo-plugins/haal-centraal"],
      "@valtimo-plugins/haal-centraal-auth": ["dist/valtimo-plugins/haal-centraal-auth"],
      "@valtimo-plugins/kvk-handelsregister": ["dist/valtimo-plugins/kvk-handelsregister"],
      "@valtimo-plugins/mtls-sslcontext": ["dist/valtimo-plugins/mtls-sslcontext"],
      "@valtimo-plugins/notify-nl": ["dist/valtimo-plugins/notify-nl"],
      "@valtimo-plugins/object-management": ["dist/valtimo-plugins/object-management"],
      "@valtimo-plugins/publictask": ["dist/valtimo-plugins/publictask"],
      "@valtimo-plugins/rotterdam-oracle-ebs": ["dist/valtimo-plugins/rotterdam-oracle-ebs"],
      "@valtimo-plugins/slack": ["dist/valtimo-plugins/slack"],
      "@valtimo-plugins/smtpmail": ["dist/valtimo-plugins/smtpmail"],
      "@valtimo-plugins/spotler": ["dist/valtimo-plugins/spotler"],
      "@valtimo-plugins/suwinet": ["dist/valtimo-plugins/suwinet"],
      "@valtimo-plugins/valtimo-llm": ["dist/valtimo-plugins/valtimo-llm"],
      "@valtimo-plugins/valtimo-ocr": ["dist/valtimo-plugins/valtimo-ocr"],
      "@valtimo-plugins/valtimo-s2t": ["dist/valtimo-plugins/valtimo-s2t"],
      "@valtimo-plugins/xential": ["dist/valtimo-plugins/xential"]
    }
  },
  "include": [
    "***.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [],
  "references": [
    {
      "path": "./src/tsconfig.app.json"
    }
  ]
}
